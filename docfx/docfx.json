{
  "metadata": [
    {
      "src": [
        {
          "src": "../",
          "files": [
            "src/AudioBand.AudioSource/AudioBand.AudioSource.csproj"
          ]
        }
      ],
      "dest": "audiosource-api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "audiosource-api/**.yml",
          "audiosource-api/index.md"
        ]
      },
      {
        "files": [
          "audioband/**.md",
          "audioband/**/toc.yml",
          "audiosource-docs/**.md",
          "audiosource-docs/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "xrefservice": [
      "https://xref.docs.microsoft.com/query?uid={uid}"
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "../docs",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "globalMetadata": {
      "_enableSearch": true
    }
  }
}