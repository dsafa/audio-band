<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:valueConverters="clr-namespace:AudioBand.Views.Wpf.ValueConverters">
    <valueConverters:StringToVisibilityConverter x:Key="StringToVisibilityConverter"/>
    <valueConverters:WinColorToWpfColorConverter x:Key="ColorConverter"/>
    <valueConverters:MultiBindingCommandParameterConverter x:Key="SelectedViewModelConverter"/>
    <valueConverters:FormatConverter x:Key="FormatConverter"/>
    <valueConverters:BoolToVisibilityConverter x:Key="BoolToVisibilityConverter"/>
    <valueConverters:EmptyStringToBoolConverter x:Key="StringToBoolConverter"/>
    <valueConverters:NumberToNumericUpDownConverter x:Key="NumericUpDownConverter"/>
    <valueConverters:ReverseConverter x:Key="ColorToStringConverter">
        <valueConverters:StringToColorConverter/>
    </valueConverters:ReverseConverter>
    <valueConverters:ColorToBrushConverter x:Key="ColorToBrushConverter"/>
    <valueConverters:StringToFontFamilyConverter x:Key="StringToFontFamilyConverter"/>
    <valueConverters:ObjectToTypeConverter x:Key="ObjectToTypeConverter"/>
</ResourceDictionary>